ts/Shop","sourcesContent":["<template>\r\n  <div class=\"shop\">\r\n      <div class=\"searchInput\">\r\n        <input type=\"text\" class=\"form-control\" placeholder=\"Search for an item..\" v-model=\"search\">\r\n      </div>\r\n      <div v-for=\"product in products\" :key=\"product._id\" class=\"cards\">\r\n          \r\n        <div class=\"card\">\r\n            <div class=\"cardImage\">\r\n                <img class=\"images\" :src=\"product.image\">\r\n                \r\n            </div>\r\n            <div class=\"cardText\">\r\n                <!-- <small> {{ product.price.toFixed(2) }}$</small>\r\n                <h2> {{ products.name }}</h2>\r\n                <p> {{ products.short }} </p> -->\r\n                <small>price:</small>\r\n                <h2>Adidas</h2>\r\n            </div>\r\n            <div class=\"cardAmount\">\r\n                <button class=\"add_btn\">Add to cart</button>\r\n                <button class=\"yourListBtn\">&hearts;</button>\r\n            </div>\r\n        </div>\r\n\r\n      </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { mapGetters } from 'vuex'\r\nexport default {\r\ndata() {\r\n    return {\r\n        \r\n        search: ''\r\n    }\r\n},\r\ncomputed: {\r\n    filteredProducts() {\r\n        return this.products.filter(product => product.name.match(this.search))\r\n    },\r\n    ...mapGetters(['products'])\r\n},\r\n\r\n\r\n}\r\n</script>\r\n\r\n<style>\r\n.shop {\r\n    background-color: #F5F5F5;\r\n}\r\n\r\n.images {\r\n    width: 200px;\r\n}\r\n\r\n.searchInput {\r\n    display: flex;\r\n}\r\n.searchInput input {\r\n    width: 55%;\r\n    margin: auto;\r\n    padding: 10px 18px;\r\n    outline: 0;\r\n    border-image: linear-gradient(to right, #121212, #121212);\r\n    border-image-slice: 1;\r\n    border-width: 0 0 1px;\r\n    background-color: #F5F5F5;\r\n    padding-top: 3rem;\r\n}\r\n\r\n.cards {\r\n    display: inline-block;\r\n    text-align: center;\r\n    margin-top: 1.5rem;\r\n}\r\n.card {\r\n    margin-top: 1rem;\r\n    border: 1px solid #121212;\r\n    width: 400px;\r\n    position: relative;\r\n    left: 25%;\r\n    border-radius: 8px;\r\n    margin: 10px;\r\n    \r\n}\r\n.card:hover {\r\n    transform: scale(1);\r\n    \r\n}\r\n.cardImage {\r\n    height: 210px;\r\n    color: #fff;\r\n    justify-content: center;\r\n    background-color: #F5F5F5;\r\n    \r\n}\r\n.cardText {\r\n    height: 190px;\r\n    /* margin-top: 1.5rem; */\r\n    /* padding-top: 1.5rem; */\r\n    padding-top: 3rem;\r\n    background-color: #F5F5F5;\r\n}\r\n\r\n.cardAmount {\r\n    height: 80px;\r\n    background-color: #121212;\r\n    text-align: center;\r\n    border-radius: 0px 0px 8px 8px;\r\n    border: none;\r\n    position: relative;\r\n}\r\n\r\n.add_btn {\r\n    width: 90px;\r\n    height: 30px;\r\n    cursor: pointer;\r\n    border: none;\r\n    background-color: #fff;\r\n    \r\n}\r\n\r\n.yourListBtn {\r\n    width: 30px;\r\n    height: 30px;\r\n    border: none;\r\n    background-color: #fff;\r\n    margin-left: 0.1rem;\r\n    cursor: pointer;\r\n}\r\n\r\n.cardAmount button {\r\n    margin-top: 1.5rem;\r\n}\r\n\r\n\r\n\r\n\r\n</style>"]}]}